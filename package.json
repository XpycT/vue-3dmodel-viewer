    "ci": "npm run lint && npm run build"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "dat.gui": "^0.7.2",
    "screenfull-es6": "^0.1.3",
    "stats.js": "^0.17.0",
